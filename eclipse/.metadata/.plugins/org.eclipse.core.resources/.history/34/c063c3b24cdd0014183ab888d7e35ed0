public class FolkUtils 
{
	public FolkData generateFolk(World world)
	{
		FolkData theFolk = EntityList.createEntityByName(fullEntityName, world);  
		
		EntityPlayer thePlayer = (EntityPlayer) world.playerEntities.get(0);
		
		theFolk.setPosition(world.); 
        world.spawnEntityInWorld(conjuredEntity); 
	}
}
